{
  "name": "myBunnings",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "lint:css": "stylelint ./src/**/*.jsx",
    "lint:eslint": "eslint --ext .js --ext .jsx src",
    "lint": "run-p lint:*",
    "test": "jest",
    "build": "webpack --config webpack-web.config.js --mode production",
    "start": "webpack-dev-server --config webpack-web.config.js --mode development --hot",
    "dev:api:webpack": "webpack --config webpack-api.config.js --mode development --watch",
    "dev:api:sam": "sam local start-api",
    "dev:api": "run-p dev:api:*",
    "deploy:api": "run-s deploy:api:clean-build deploy:api:build deploy:api:upload-package deploy:api:deploy-stack",
    "deploy:api:clean-build": "rm -rf ./.aws-sam",
    "deploy:api:build": "webpack --config webpack-api.config.js --mode production",
    "deploy:api:upload-package": "sam package --output-template-file ./.aws-sam/template.yaml --s3-bucket mybunnings --s3-prefix packages",
    "deploy:api:deploy-stack": "sam deploy --template-file ./.aws-sam/template.yaml --stack-name mybunnings --capabilities CAPABILITY_IAM"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@zxing/library": "^0.16.0",
    "aws-sdk": "^2.642.0",
    "chart.js": "^2.9.3",
    "cheerio": "^1.0.0-rc.3",
    "prop-types": "^15.7.2",
    "react": "^16.13.0",
    "react-chartjs-2": "^2.9.0",
    "react-dom": "^16.13.0",
    "react-hot-loader": "^4.12.20",
    "react-router-dom": "^5.1.2",
    "styled-components": "^4.4.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-proposal-do-expressions": "^7.8.3",
    "@babel/plugin-proposal-export-default-from": "^7.8.3",
    "@babel/plugin-proposal-export-namespace-from": "^7.8.3",
    "@babel/plugin-proposal-function-bind": "^7.8.3",
    "@babel/plugin-proposal-function-sent": "^7.8.3",
    "@babel/plugin-proposal-json-strings": "^7.8.3",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.8.3",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.8.3",
    "@babel/plugin-proposal-numeric-separator": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.8.3",
    "@babel/plugin-proposal-pipeline-operator": "^7.8.3",
    "@babel/plugin-proposal-throw-expressions": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-import-meta": "^7.8.3",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-react": "^7.8.3",
    "@hot-loader/react-dom": "^16.13.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "css-loader": "^2.1.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^22.21.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "npm-run-all": "^4.1.5",
    "react-test-renderer": "^16.13.0",
    "style-loader": "^0.23.1",
    "stylelint": "^10.1.0",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-config-styled-components": "^0.1.1",
